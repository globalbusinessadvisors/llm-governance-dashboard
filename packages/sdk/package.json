{
  "name": "@llm-dev-ops/llm-governance-sdk",
  "version": "1.0.1",
  "description": "TypeScript/JavaScript SDK for LLM Governance Dashboard API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "llm",
    "governance",
    "api",
    "sdk",
    "typescript",
    "client"
  ],
  "author": "LLM Dev Ops Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/globalbusinessadvisors/llm-governance-dashboard.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/globalbusinessadvisors/llm-governance-dashboard/tree/main/packages/sdk",
  "bugs": {
    "url": "https://github.com/globalbusinessadvisors/llm-governance-dashboard/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@llm-dev-ops/llm-governance-types": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {},
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
